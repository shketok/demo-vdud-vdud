<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="debug" verbose="1">

  <test name="Test unsuccessful password restore with unregistered email">
    <classes>
      <class name="ru.vdudvdud.tests.regression.account.restore.UnsuccessfulPasswordRestoreWithUnregisteredEmailTest"/>
    </classes>
  </test>

  <test name="Test unsuccessful password restore with incorrect email">
    <classes>
      <class name="ru.vdudvdud.tests.regression.account.restore.UnsuccessfulPasswordRestoreWithIncorrectEmailTest"/>
    </classes>
  </test>

  <test name="Test successful password restore">
    <classes>
      <class name="ru.vdudvdud.tests.regression.account.restore.SuccessfulPasswordRestore"/>
    </classes>
  </test>

  <test name="Test unsuccessful password restore with different passwords">
    <classes>
      <class name="ru.vdudvdud.tests.regression.account.restore.UnsuccessfulPasswordRestoreWithDifferentPasswordsTest"/>
    </classes>
  </test>

  <test name="Test unsuccessful password restore with empty passwords">
    <classes>
      <class name="ru.vdudvdud.tests.regression.account.restore.UnsuccessfulPasswordRestoreWithEmptyPasswordsTest"/>
    </classes>
  </test>
</suite>